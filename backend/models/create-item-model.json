{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "create-item",
  "type": "object",
  "properties": {
    "ItemName": {
      "type": "string",
      "minLength": 3,
      "maxLength": 20
    },
    "startDate": {
      "type": "string",
      "pattern": "[0-9]{4}-[0-9]{2}-[0-9]{2}"
    },
    "endDate": {
      "type": "string",
      "pattern": "[0-9]{4}-[0-9]{2}-[0-9]{2}"
    }
  },
  "required": [
    "ItemName",
    "startDate"
  ],
  "additionalProperties": false
}
